{
    "name": "Unlock Table and Verify Status",
    "prerequisite": {
      "method": "POST",
      "url": "http://localhost:3000/api/tables/lock",
      "header": [{ "key": "Content-Type", "value": "application/json" }],
      "body": {
        "mode": "raw",
        "raw": "{\"tableId\": \"table-123\", \"userId\": \"user-abc\", \"duration\": 600}"
      }
    },
    "request": {
      "method": "POST",
      "url": "http://localhost:3000/api/tables/unlock",
      "header": [{ "key": "Content-Type", "value": "application/json" }],
      "body": {
        "mode": "raw",
        "raw": "{\"tableId\": \"table-123\", \"userId\": \"user-abc\"}"
      }
    },
    "response": {
      "status": "OK",
      "code": 200,
      "header": [{ "key": "Content-Type", "value": "application/json" }],
      "body": "{\"success\": true, \"message\": \"Table unlocked successfully\"}"
    },
    "followUpRequest": {
      "method": "GET",
      "url": "http://localhost:3000/api/tables/table-123/status",
      "header": [],
      "response": {
        "status": "OK",
        "code": 200,
        "header": [{ "key": "Content-Type", "value": "application/json" }],
        "body": "{\"isLocked\": false}"
      }
    }
  }